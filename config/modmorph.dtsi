hm: homerow_mods {
  compatible = "zmk,behavior-hold-tap";
  label = "HOMEROW_MODS";
  #binding-cells = <2>;
  tapping-term-ms = <200>;
  quick_tap_ms = <175>;
  flavor = "tap-preferred";
  bindings = <&kp>, <&kp>;
};

gresc: grave_escape {
  compatible = "zmk,behavior-mod-morph";
  #binding-cells = <0>;
  bindings = <&kp ESC>, <&kp GRAVE>;
  mods = <(MOD_LGUI|MOD_LSFT|MOD_RGUI|MOD_RSFT)>;
};

